alias vi="vim"
alias ll="ls -la"
alias cdp="cd /var/www/html"
alias cdml="cd /var/log"
alias cdl="cd /var/log/"
alias cdlp="cd /var/log/php"
alias cdlh="cd /var/log/httpd"
alias log="tail -f /var/log/php/error.log"
alias phpinfo="echo  \"<?php phpinfo(); \" | php "
alias reload="source ~/.bashrc"
alias dump="composer dumpautoload"
alias gs="git status"
alias gst="git status"
alias las="ls -latr"
alias pbcopy="xclip -selection clipboard"
alias pbpaste="xclip -selection clipboard -o"
BAT_PAGER=never
